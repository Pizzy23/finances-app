generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Register {
  id       Int    @id @default(autoincrement())
  name     String
  email    String @unique
  password String
}

model Auth {
  id    Int    @id @default(autoincrement())
  email String @unique
  token String
}

model Login {
  id             Int     @id @default(autoincrement())
  email          String  @unique
  isLogged       Boolean
  isValidAccount Boolean
}
